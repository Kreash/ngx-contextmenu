{
  "name": "@kreash/ngx-contextmenu-repository",
  "version": "18.1.0",
  "description": "An Angular component to show a context menu on an arbitrary component",
  "keywords": [
    "contextmenu",
    "ngx-contextmenu",
    "angular",
    "angular-contextmenu",
    "angular2",
    "angular2-contextmenu",
    "ngx",
    "ng2",
    "ng2-contextmenu"
  ],
  "author": "Kreash <anton.kreash@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kreash/ngx-contextmenu.git"
  },
  "main": "lib/index.js",
  "module": "lib/index.js",
  "jsnext:main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:lib && npm run build:demo",
    "build:demo": "rimraf dist/demo && ng build",
    "build:lib": "ng build ngx-contextmenu && copyfiles README.md LICENSE dist/ngx-contextmenu/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pub": "npm run pub:lib && npm run pub:demo",
    "pub:demo": "npm run build:demo -- --base-href /ngx-contextmenu/ && gh-pages -d dist/demo",
    "pub:lib": "npm run build:lib && npm publish ./dist/ngx-contextmenu",
    "pack:lib": "npm run build:lib && cd dist/ngx-contextmenu && npm pack && copyfiles *.tgz ../../ && cd ../.. && rimraf dist/ngx-contextmenu/*.tgz"
  },
  "private": false,
  "devDependencies": {
    "@angular-devkit/build-angular": "18.1.0",
    "@angular/cdk": "18.1.0",
    "@angular/common": "18.1.0",
    "@angular/core": "18.1.0",
    "@angular/cli": "18.1.0",
    "@angular/compiler": "18.1.0",
    "@angular/compiler-cli": "18.1.0",
    "@angular/forms": "18.1.0",
    "@angular/platform-browser": "18.1.0",
    "@angular/platform-browser-dynamic": "18.1.0",
    "@angular/router": "18.1.0",
    "ng-packagr": "^18.1.0",
    "@types/node": "14.6.0",
    "codelyzer": "^6.0.0",
    "copyfiles": "^2.3.0",
    "gh-pages": "^3.1.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "protractor": "^7.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.2",
    "ts-node": "~8.10.2",
    "tslib": "^2.0.0",
    "tslint": "~6.1.3",
    "typescript": "~5.4.5",
    "zone.js": "~0.14.4"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  }
}
